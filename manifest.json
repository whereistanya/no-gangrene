{
"name":"No Gangrene",
"description":"Hide pictures of medical conditions on wikipedia",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"],
      "js": ["nogangrene.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["alpaca.jpg"]
}
